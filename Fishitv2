-- ===================== EVENT SECTION =====================
local EventStatus = PageEvent:CreateLabel("Event • idle")

-- Kumpulan nama awal (boleh kosong; kita bisa Refresh otomatis dari dunia)
local seedEvents = { "Shark Hunt","Ghost Shark Hunt","Worm Hunt","Black Hole","Shocked","Ghost Worm","Meteor Rain","Megalodon Hunt","Admin Event" }

-- Helper: ambil daftar event dari dunia
local function discoverEvents()
    local found, set = {}, {}
    local props = workspace:FindFirstChild("Props")
    local containers = {workspace}
    if props then table.insert(containers, 1, props) end
    for _,root in ipairs(containers) do
        for _,obj in ipairs(root:GetDescendants()) do
            if obj:IsA("Model") then
                local nm = tostring(obj.Name)
                if #nm>=3 then
                    -- heuristik nama event umum
                    local low = string.lower(nm)
                    if low:find("hunt") or low:find("worm") or low:find("meteor") or low:find("black") or low:find("shock") or low:find("event") then
                        set[nm]=true
                    end
                end
            end
        end
    end
    -- gabungkan seed agar user tetap bisa pilih manual
    for _,n in ipairs(seedEvents) do set[n]=true end
    for k in pairs(set) do table.insert(found,k) end
    table.sort(found)
    return found
end

-- Multi-select events
local selectedSet = {}
local ms = PageEvent:CreateMultiSelect("Select Events", discoverEvents(), function(selSet)
    selectedSet = selSet
    local n=0 for _ in pairs(selectedSet) do n+=1 end
    EventStatus:UpdateLabel(("Event • %d selected"):format(n))
end)

-- Tombol kecil untuk refresh daftar (pakai spacing bawaan card)
PageEvent:CreateButton("Refresh Event List","Scan ulang dunia & update daftar",function()
    ms:SetOptions(discoverEvents())
    EventStatus:UpdateLabel("Event • list refreshed")
end)

-- Toggle Auto Teleport
PageEvent:CreateToggle("Auto Teleport Selected Events","Otomatis ke event aktif yang dipilih",function(on)
    state.autoEventTP = on
    EventStatus:UpdateLabel(on and "Event • AutoTP: ON" or "Event • AutoTP: OFF")
end)

-- Teleport logic
local function getPivotForEventModel(evModel)
    if not evModel then return nil end
    local boat = evModel:FindFirstChild("Fishing Boat")
    if boat and boat.GetPivot then return boat:GetPivot() end
    if evModel.GetPivot then return evModel:GetPivot() end
    local pp = evModel.PrimaryPart
    return pp and pp.CFrame or nil
end

local function findEventModelByName(name)
    if not name then return nil end
    local props = workspace:FindFirstChild("Props")
    if props then
        local m = props:FindFirstChild(name)
        if m then return m end
    end
    -- fallback: cari di seluruh workspace (case-insensitive)
    local targetLow = string.lower(name)
    for _,m in ipairs(workspace:GetDescendants()) do
        if m:IsA("Model") and string.lower(m.Name) == targetLow then
            return m
        end
    end
    return nil
end

-- Loop AutoTP
task.spawn(function()
    while true do
        if state.autoEventTP then
            local now=os.clock()
            if now - state.lastEventTP > 2.0 then
                -- ambil satu event yang sedang ada modelnya
                local chosenCF
                for name,_ in pairs(selectedSet) do
                    local mdl = findEventModelByName(name)
                    local cf  = getPivotForEventModel(mdl)
                    if cf then chosenCF = cf; EventStatus:UpdateLabel("Event • active: "..name) break end
                end
                if chosenCF then
                    state.lastEventTP = now
                    tpTo(chosenCF.Position + Vector3.new(0,15,0))
                else
                    EventStatus:UpdateLabel("Event • tidak ditemukan yang aktif")
                end
            end
        end
        task.wait(0.4)
    end
end)

-- Live HUD + Coordinates
RS.RenderStepped:Connect(function()
    local rod=getEquippedRodName() or "?"
    local c,r=effDelays()
    pcall(function() if Live and Live.UpdateLabel then Live:UpdateLabel(("Rod: %s  • Reel: %.2fs  • Cmpl: %.2fs  • Mode: %s"):format(rod,r,c,state.blatant and "blatant" or "normal")) end end)
end)

RS.RenderStepped:Connect(function()
    local ch = LocalPlayer.Character or LocalPlayer.CharacterAdded:Wait()
    local hrp = ch and ch:FindFirstChild("HumanoidRootPart")
    if hrp and CoordLabel and CoordLabel.UpdateLabel then
        local p = hrp.Position
        CoordLabel:UpdateLabel(("Coordinates • X: %.1f  Y: %.1f  Z: %.1f"):format(p.X,p.Y,p.Z))
    end
end)
